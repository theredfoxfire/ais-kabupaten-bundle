<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="ais_kabupaten.kabupaten.handler.class">Ais\KabupatenBundle\Handler\KabupatenHandler</parameter>
        <parameter key="ais_kabupaten.kabupaten.class">Ais\KabupatenBundle\Entity\Kabupaten</parameter>
    </parameters>

    <services>
        <service id="ais_kabupaten.kabupaten.handler" class="%ais_kabupaten.kabupaten.handler.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%ais_kabupaten.kabupaten.class%</argument>
            <argument type="service" id="form.factory"></argument>
        </service>
        
        <service id="ais_kabupaten.kabupaten.manager" class="Doctrine\ORM\EntityManager" factory-service="doctrine" factory-method="getManagerForClass">
			<argument>%ais_kabupaten.kabupaten.class%</argument>
		</service>
        
        <service id="ais_kabupaten.kabupaten.repository" 
			class="Ais\KabupatenBundle\Repository\KabupatenRepository"
			factory-service="ais_kabupaten.kabupaten.manager" factory-method="getRepository">
				<argument>%ais_kabupaten.kabupaten.class%</argument>
		</service>
    </services>

</container>
